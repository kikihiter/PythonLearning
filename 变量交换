    #python 可以在同一行内直接交换两个变量的值
    a,b=0,1;
    print (a,b)
    #0 1
    a,b=b,a
    print (a,b)
    #1 0
    
    #通常上，变量交换不可以直接如下，会发生错误
    c,d=0,1
    print (c,d)
    #0 1
    c=d
    d=c
    print (c,d)
    #1 1
    
    #一般需要一个中间变量来进行传递
    e,f=0,1
    print (e,f)
    #0 1
    g=e
    e=f
    f=g
    print (e,f)
    #1 0
    
    #python在同一行直接可以进行交换，方便了许多
